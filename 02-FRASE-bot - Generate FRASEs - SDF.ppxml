							<sci:data object="SciTegic.EvaluateExpression.1">
								<sci:component name="Custom Manipulator (PilotScript)" version="2">
									<sci:arg name="Expression" type="ExpressionType" required="true" visibility="visible" multi="false" expanded="true" implbase="{0995D675-7EE2-432C-95DF-E91C14052B22}">
										<sci:help>Expression (or list of expressions separated by semi-colons) to evaluate.</sci:help>
										<sci:member>Initial Expression</sci:member>
										<sci:member>Final Expression</sci:member>
										<sci:member>Keep Calculated Properties</sci:member>
										<sci:value>#CenterXYZ := @FRASECenterXYZ;
Expand(&apos;,&apos;,#CenterXYZ);
IsClose := false;
FOR #i IN 1 .. MolNumAtoms()
LOOP
	FOR #j IN 1 .. Numvalues(@LigX)
	LOOP
		#XYZ := AtomXYZ(#i);
		#D := sqrt(Power(#XYZ[1]-#CenterXYZ[1]-@LigX[#j],2)+Power(#XYZ[2]-#CenterXYZ[2]-@LigY[#j],2)+Power(#XYZ[3]-#CenterXYZ[3]-@LigZ[#j],2));
		IF #D &lt; 4.5 THEN
			#MeanX := 0;
			#MeanY := 0;
			#MeanZ := 0;
			#NumAtoms := MolNumAtoms();
			FOR #i IN 1 .. #NumAtoms
			LOOP
				#XYZ := AtomXYZ(#i);
				AtomSetXYZ(#i,#XYZ[1]-#CenterXYZ[1],#XYZ[2]-#CenterXYZ[2],#XYZ[3]-#CenterXYZ[3]);
				#MeanX := #MeanX + #XYZ[1]-#CenterXYZ[1];
				#MeanY := #MeanY + #XYZ[2]-#CenterXYZ[2];
				#MeanZ := #MeanZ + #XYZ[3]-#CenterXYZ[3];
				IF AtomLabel(#i) like &apos;%CA%&apos; THEN
					#CaX := #XYZ[1]-#CenterXYZ[1];
					#CaY := #XYZ[2]-#CenterXYZ[2];
					#CaZ := #XYZ[3]-#CenterXYZ[3];
				END IF;
			END LOOP;
			ResMeanXYZ := format(&apos;%5.2f&apos;,#MeanX/#NumAtoms) . &apos;,&apos; . format(&apos;%5.2f&apos;,#MeanY/#NumAtoms) . &apos;,&apos; . format(&apos;%5.2f&apos;,#MeanZ/#NumAtoms);
			IF #CaX is defined THEN
				ResCaXYZ := format(&apos;%5.2f&apos;,#CaX) . &apos;,&apos; . format(&apos;%5.2f&apos;,#CaY) . &apos;,&apos; . format(&apos;%5.2f&apos;,#CaZ);
			END IF;
			IsClose := true;
			BREAK;
		END IF;
	END LOOP;
	IF IsClose THEN
		BREAK;
	END IF;
END LOOP;
PDB_ID := @PDB_ID;
Remove(&apos;ResidueType&apos;);
</sci:value>
