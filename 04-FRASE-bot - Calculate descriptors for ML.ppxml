								<sci:member>Keep Calculated Properties</sci:member>
								<sci:value>//if @HasLigFragment eq true then
	//Resize(LigAtomInteractions,0);
	Resize(LigAtomID,0);
//	PDB_ID := @PDB_ID;
//	FRASE_ID := @FRASE_ID;
	TrueFRASE := @TrueFRASE;
	Reorder(&apos;LigAtomID&apos;,&apos;TrueFRASE&apos;);
	for #i in 1 .. Numvalues(@ithLigAtomTypes)
	loop
	//	Resize(#CurrentInteractions,0);
		for #j in 1 .. Numvalues(@InteractionType)
		loop
	//		Append(#CurrentInteractions,@ithLigAtomInteractions[#i][#j]);
			#prop := format(&apos;i%03d&apos;,#j);
			Append(Property(#prop), @ithLigAtomInteractions[#i][#j]);
		end loop;
	//	Contract(&apos;,&apos;,#CurrentInteractions);
	//	Append(LigAtomInteractions,#CurrentInteractions);
		Append(LigAtomID, @FRASE_ID . &apos;_&apos; . #i);
	end loop;
//end if
</sci:value>
