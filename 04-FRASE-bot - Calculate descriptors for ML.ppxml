Resize(ProtTypes,0);
Resize(ProtXYZ,0);
for #i in 1 .. MolNumAtoms()
loop
	Resize(#CurrentAtomTypes,0);
	if AtomTypeString(#i) eq &apos;C&apos; AND AtomIs(#i,&apos;Aromatic&apos;) then
		Append(#CurrentAtomTypes,&apos;AromaticCarbon&apos;);
	end if;
	if AtomTypeString(#i) eq &apos;C&apos; AND AtomHybridization(#i) eq &apos;sp3&apos; then
		Append(#CurrentAtomTypes,&apos;AliphaticCarbon&apos;);
	end if;
	if AtomTypeString(#i) eq &apos;N&apos; AND AtomIs(#i,&apos;Aromatic&apos;) then
		Append(#CurrentAtomTypes,&apos;AromaticNitrogen&apos;);
	end if;
	if AtomIs(#i,&apos;NegativeIonizable&apos;) then
		Append(#CurrentAtomTypes,&apos;NegativeIonizable&apos;);
	end if;
	if AtomIs(#i,&apos;PositiveIonizable&apos;) then
		Append(#CurrentAtomTypes,&apos;PositiveIonizable&apos;);
	end if;
	if AtomIs(#i,&apos;HbondDonor&apos;) then
		Append(#CurrentAtomTypes,&apos;HbondDonor&apos;);
	end if;
	if AtomIs(#i,&apos;HbondAcceptor&apos;) then
		Append(#CurrentAtomTypes,&apos;HbondAcceptor&apos;);
	end if;
	if AtomIs(#i,&apos;AmideNitrogen&apos;) then
		Append(#CurrentAtomTypes,&apos;AmideNitrogen&apos;);
	end if;
	if AtomIs(#i,&apos;AmineNitrogen&apos;) then
		Append(#CurrentAtomTypes,&apos;AmineNitrogen&apos;);
	end if;
	if AtomIs(#i,&apos;CarboxylateOxygen&apos;) then
		Append(#CurrentAtomTypes,&apos;CarboxylateOxygen&apos;);
	end if;
	if AtomIs(#i,&apos;AlcoholOxygen&apos;) then
		Append(#CurrentAtomTypes,&apos;AlcoholOxygen&apos;);
	end if;
	if AtomIs(#i,&apos;ImineNitrogen&apos;) then
		Append(#CurrentAtomTypes,&apos;ImineNitrogen&apos;);
	end if;
	ProtTypes := #CurrentAtomTypes;
	for #j in 1 .. Numvalues(#CurrentAtomTypes)
	loop
		for #k in 1 .. Numvalues(@ithLigAtomTypes)
		loop
			#kthLigAtomTypes := @ithLigAtomTypes[#k];
			Expand(&apos;,&apos;,#kthLigAtomTypes);
			for #l in 1 .. Numvalues(#kthLigAtomTypes)
			loop
				if #kthLigAtomTypes[#l] ne &apos;&apos; then
					#IntIndex := HashFind(@InteractionType,#kthLigAtomTypes[#l] . &apos;_&apos; . #CurrentAtomTypes[#j]);
					#ligcoord := @ithLigAtomCoord[#k];
					Expand(&apos;,&apos;,#ligcoord);
					#protcoord := AtomXYZ(#i);
					Append(ProtXYZ,#protcoord);
					Expand(&apos;,&apos;,#protcoord);
					#r := sqrt(power(#ligcoord[1]-#protcoord[1],2)+power(#ligcoord[2]-#protcoord[2],2)+power(#ligcoord[3]-#protcoord[3],2));
					if #r &lt;= 3 then
						#dE := exp(-power(#r-3,2)/1);
					end if;
					if #r &gt; 3 and #r &lt;= 10 then
						#dE := exp(-power(#r-3,2)/6);
					end if;
					if #r &gt; 10 then
						#dE := 0;
					end if;
					@ithLigAtomInteractions[#k][#IntIndex] := @ithLigAtomInteractions[#k][#IntIndex] + #dE;
				end if;
			end loop;
		end loop;
	end loop;
end loop;
</sci:value>
