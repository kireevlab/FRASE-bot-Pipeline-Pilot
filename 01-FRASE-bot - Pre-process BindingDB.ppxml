			<sci:data object="SciTegic.EvaluateExpression.1">
				<sci:component name="Custom Manipulator (PilotScript)" version="2">
					<sci:arg name="Expression" type="ExpressionType" required="true" visibility="visible" multi="false" expanded="true" implbase="{0995D675-7EE2-432C-95DF-E91C14052B22}">
						<sci:help>Expression (or list of expressions separated by semi-colons) to evaluate.</sci:help>
						<sci:member>Initial Expression</sci:member>
						<sci:member>Final Expression</sci:member>
						<sci:member>Keep Calculated Properties</sci:member>
						<sci:value>TEXT_SPLIT:=rsplit(Text, &apos;\n&apos;);
Remove(&apos;Text&apos;);
Resize(SDF_SPLIT,0);
#i := 1;
WHILE #i &lt;= Numvalues(TEXT_SPLIT)
LOOP
	IF #i eq 1 AND FindSubstring(TEXT_SPLIT[#i], &apos;Mrv&apos;) &gt; 1 THEN
		#Mrv_pos := FindSubstring(TEXT_SPLIT[#i], &apos;Mrv&apos;);
		Append(SDF_SPLIT,Substring(TEXT_SPLIT[#i],1,#Mrv_pos-2));
		Append(SDF_SPLIT,Substring(TEXT_SPLIT[#i],#Mrv_pos));
		#i := #i + 1;
	ELSIF #i &gt; 1 AND TEXT_SPLIT[#i] like &apos;&gt; &lt;BindingDB%Target%Chain%Sequence%&apos;
	OR TEXT_SPLIT[#i] like &apos;&gt; &lt;PDB%ID%of%Target%Chain%&apos; 
	OR TEXT_SPLIT[#i] like &apos;&gt; &lt;UniProt%SwissProt%Target%Chain%&apos;
	OR TEXT_SPLIT[#i] like &apos;&gt; &lt;UniProt%TrEMBL%Target%Chain%&apos;
	THEN
		#i := #i + 1;
		WHILE NOT(TEXT_SPLIT[#i] like &apos;&gt; &lt;%&apos;) AND #i &lt; Numvalues(TEXT_SPLIT)
		LOOP
			#i := #i + 1;
		END LOOP;
	ELSE
		Append(SDF_SPLIT,TEXT_SPLIT[#i]);
		#i := #i + 1;
	END IF;
END LOOP;
Remove(&apos;TEXT_SPLIT&apos;);
//Rename(&apos;PDB ID(s) for Ligand-Target Complex&apos;,&apos;PDBID&apos;);
</sci:value>
