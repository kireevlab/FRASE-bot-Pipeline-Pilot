									<sci:arg name="Expression" type="ExpressionType" required="true" visibility="visible" multi="false" expanded="true" implbase="{0995D675-7EE2-432C-95DF-E91C14052B22}">
										<sci:help>Expression (or list of expressions separated by semi-colons) to evaluate.</sci:help>
										<sci:member>Initial Expression</sci:member>
										<sci:member>Final Expression</sci:member>
										<sci:member>Keep Calculated Properties</sci:member>
										<sci:value>#MeanX := 0;
#MeanY := 0;
#MeanZ := 0;
#NumAtoms := MolNumAtoms();
FOR #i IN 1 .. #NumAtoms
LOOP
	#XYZ := AtomXYZ(#i);
	#MeanX := #MeanX + #XYZ[1];
	#MeanY := #MeanY + #XYZ[2];
	#MeanZ := #MeanZ + #XYZ[3];
END LOOP;
Resize(@FRASECenterXYZ,0);
@FRASECenterXYZ := format(&apos;%5.2f&apos;,#MeanX/#NumAtoms) . &apos;,&apos; . format(&apos;%5.2f&apos;,#MeanY/#NumAtoms) . &apos;,&apos; . format(&apos;%5.2f&apos;,#MeanZ/#NumAtoms);
ResMeanXYZ := format(&apos;%5.2f&apos;,#MeanX/#NumAtoms) . &apos;,&apos; . format(&apos;%5.2f&apos;,#MeanY/#NumAtoms) . &apos;,&apos; . format(&apos;%5.2f&apos;,#MeanZ/#NumAtoms);
ResCaXYZ := ResMeanXYZ;
Resize(@LigX,0);
Resize(@LigY,0);
Resize(@LigZ,0);
Resize(@Heavy,0);
FOR #i IN 1 .. MolNumAtoms()
LOOP
	#XYZ := AtomXYZ(#i);
	Append(@LigX,#XYZ[1]);//-#MeanX/#NumAtoms);	
	Append(@LigY,#XYZ[2]);//-#MeanY/#NumAtoms);
	Append(@LigZ,#XYZ[3]);//-#MeanZ/#NumAtoms);
	IF AtomIsHydrogen(#i) THEN
		Append(@Heavy, false);
	ELSE
		Append(@Heavy, true);
	END IF;
//	AtomSetXYZ(#i,#XYZ[1]-#MeanX/#NumAtoms,#XYZ[2]-#MeanY/#NumAtoms,#XYZ[3]-#MeanZ/#NumAtoms);
//	AtomSetXYZ(#i,#XYZ[1],#XYZ[2],#XYZ[3]);
END LOOP;
//-22.3738    4.2639   -9.0129
//DtoT:=sqrt(power(#MeanX/#NumAtoms+22.37,2)+power(#MeanY/#NumAtoms-4.26,2)+power(#MeanZ/#NumAtoms+9.01,2));
</sci:value>
