					<sci:data object="SciTegic.EvaluateExpression.1">
						<sci:component name="Custom Manipulator (PilotScript)" version="2">
							<sci:arg name="Expression" type="ExpressionType" required="true" visibility="visible" multi="false" expanded="true" implbase="{0995D675-7EE2-432C-95DF-E91C14052B22}">
								<sci:help>Expression (or list of expressions separated by semi-colons) to evaluate.</sci:help>
								<sci:member>Initial Expression</sci:member>
								<sci:member>Final Expression</sci:member>
								<sci:member>Keep Calculated Properties</sci:member>
								<sci:value>// Convert strings to arrays
// t_f_xyz and t_f_xyz contain cartesian coordinates of residue triplets from, respectively, the FRASE and the target
// t_f_xyz[1] to t_f_xyz[3] contain xyz for the first residue of the triplet; positions 4-6 are occupied by xyz for the 2nd residue; 7-9 for the third
// A residue&apos;s coordinates are coordinates of its alpha carbon (CA)
t_f_xyz := HashFind(@FRASE_xyz_hash,FRASE_ID);
Expand(&apos;,&apos;,t_f_xyz);

// Create fragments for triplets
MolFromText(&apos;SMILES&apos;,&apos;C1SO1&apos;);
AtomSetXYZ(1, t_f_xyz[1], t_f_xyz[2], t_f_xyz[3]);
AtomSetXYZ(2, t_f_xyz[4], t_f_xyz[5], t_f_xyz[6]);
AtomSetXYZ(3, t_f_xyz[7], t_f_xyz[8], t_f_xyz[9]);
MolSetInfo(&apos;CoordDimension&apos;, 3);
IsFRASE:=&apos;IsFRASE&apos;;

Remove(&apos;NumberOfClosest&apos;,&apos;ClosestSimilarities&apos;);
</sci:value>
							</sci:arg>
