										<sci:value>#CenterXYZ := @FRASECenterXYZ;
BadContacts := 0;
GoodContacts := 0;
//Resize(Dlp,0);
Expand(&apos;,&apos;,#CenterXYZ);
FOR #i IN 1 .. MolNumAtoms()
LOOP
	IF AtomIsHydrogen(#i) ne true THEN
		#XYZ := AtomXYZ(#i);
		FOR #j IN 1 .. Numvalues(@LigX)
		LOOP
			IF @Heavy[#j] THEN
				#D := sqrt(Power(#XYZ[1]-@LigX[#j],2)+Power(#XYZ[2]-@LigY[#j],2)+Power(#XYZ[3]-@LigZ[#j],2));
				IF #D &lt; 2 THEN
					BadContacts := BadContacts+1;
				END IF;
				IF #D &gt;= 2 AND #D &lt; 4.5 THEN
					GoodContacts := GoodContacts+1;
				END IF;
			END IF;
		END LOOP;
	END IF;
END LOOP;
Num_ligatm := Numvalues(@LigX);

</sci:value>
