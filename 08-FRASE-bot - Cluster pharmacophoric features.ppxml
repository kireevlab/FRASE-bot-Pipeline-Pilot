			<sci:data object="SciTegic.EvaluateExpression.1">
				<sci:component name="Custom Manipulator (PilotScript)" version="2">
					<sci:arg name="Expression" type="ExpressionType" required="true" visibility="visible" multi="false" expanded="true" implbase="{0995D675-7EE2-432C-95DF-E91C14052B22}">
						<sci:help>Expression (or list of expressions separated by semi-colons) to evaluate.</sci:help>
						<sci:member>Initial Expression</sci:member>
						<sci:member>Final Expression</sci:member>
						<sci:member>Keep Calculated Properties</sci:member>
						<sci:value>Resize(#Aromatic,0);
Resize(#AromaticXYZ,0);
Resize(Feature,0);
Resize(FeatureXYZ,0);
Resize(#Hydrophobic,0);
for #i in 1 .. MolNumAtoms()
loop
	if AtomIs(#i,&apos;Aromatic&apos;) then
		Append(#Aromatic,#i);
		#CurrentCoord := AtomXYZ(#i);
		Contract(&apos;,&apos;,#CurrentCoord);
		Append(#AromaticXYZ,#CurrentCoord);
	end if;
	if AtomTypeString(#i) eq &apos;C&apos; AND AtomHybridization(#i) eq &apos;sp3&apos; then
		if not(Contains(#Hydrophobic,#i)) then
			#NumHydrophobicNeigjbours:=0;
			for #j in 1 .. AtomNumAtt(#i)
			loop
				if not(Contains(#Hydrophobic,AtomAttAtom(#i,#j))) then
					if (AtomTypeString(AtomAttAtom(#i,#j)) eq &apos;C&apos; AND AtomHybridization(AtomAttAtom(#i,#j)) eq &apos;sp3&apos;) OR AtomTypeString(AtomAttAtom(#i,#j)) eq &apos;F&apos; OR AtomTypeString(AtomAttAtom(#i,#j)) eq &apos;Cl&apos; OR AtomTypeString(AtomAttAtom(#i,#j)) eq &apos;Br&apos; then
						#NumHydrophobicNeigjbours:=#NumHydrophobicNeigjbours+1;
					end if;
				end if;
			end loop;
			if #NumHydrophobicNeigjbours &gt; 1 then
				Append(#Hydrophobic,#i);
				for #j in 1 .. AtomNumAtt(#i)
				loop
					if not(Contains(#Hydrophobic,AtomAttAtom(#i,#j))) then
						if (AtomTypeString(AtomAttAtom(#i,#j)) eq &apos;C&apos; AND AtomHybridization(AtomAttAtom(#i,#j)) eq &apos;sp3&apos;) OR AtomTypeString(AtomAttAtom(#i,#j)) eq &apos;F&apos; OR AtomTypeString(AtomAttAtom(#i,#j)) eq &apos;Cl&apos; OR AtomTypeString(AtomAttAtom(#i,#j)) eq &apos;Br&apos; then
							Append(#Hydrophobic,AtomAttAtom(#i,#j));
						end if;
					end if;
				end loop;
				Append(Feature,&apos;Hydrophobic&apos;);
				#CurrentCoord := AtomXYZ(#i);
				Contract(&apos;,&apos;,#CurrentCoord);
				Append(FeatureXYZ,#CurrentCoord);
				Append(FeatureSmiles,&apos;C&apos;);
			end if;
		end if;
	end if;
	if AtomIs(#i,&apos;HbondAcceptor&apos;) then
		Append(Feature,&apos;HbondAcceptor&apos;);
		#CurrentCoord := AtomXYZ(#i);
		Contract(&apos;,&apos;,#CurrentCoord);
		Append(FeatureXYZ,#CurrentCoord);
		Append(FeatureSmiles,&apos;C&apos;);
	end if;
	if AtomIs(#i,&apos;HbondDonor&apos;) then
		Append(Feature,&apos;HbondDonor&apos;);
		#CurrentCoord := AtomXYZ(#i);
		Contract(&apos;,&apos;,#CurrentCoord);
		Append(FeatureXYZ,#CurrentCoord);
		Append(FeatureSmiles,&apos;C&apos;);
	end if;
	if AtomIs(#i,&apos;NegativeIonizable&apos;) then
		Append(Feature,&apos;NegativeIonizable&apos;);
		#CurrentCoord := AtomXYZ(#i);
		Contract(&apos;,&apos;,#CurrentCoord);
		Append(FeatureXYZ,#CurrentCoord);
		Append(FeatureSmiles,&apos;C&apos;);
	end if;
	if AtomIs(#i,&apos;PositiveIonizable&apos;) then
		Append(Feature,&apos;PositiveIonizable&apos;);
		#CurrentCoord := AtomXYZ(#i);
		Contract(&apos;,&apos;,#CurrentCoord);
		Append(FeatureXYZ,#CurrentCoord);
		Append(FeatureSmiles,&apos;C&apos;);
	end if;
end loop;
if Numvalues(#Aromatic) &gt; 0 then
	#meanX := 0;
	#meanY := 0;
	#meanZ := 0;
	for #i in 1 .. Numvalues(#Aromatic)
	loop
		#XYZ := #AromaticXYZ[#i];
		Expand(&apos;,&apos;,#XYZ);
		#meanX := #meanX + #XYZ[1];
		#meanY := #meanY + #XYZ[2];
		#meanZ := #meanZ + #XYZ[3];
	end loop;
	#meanX := #meanX/Numvalues(#Aromatic);
	#meanY := #meanY/Numvalues(#Aromatic);
	#meanZ := #meanZ/Numvalues(#Aromatic);
	Append(Feature,&apos;Aromatic&apos;);
	Append(FeatureXYZ,#meanX . &apos;,&apos; . #meanY . &apos;,&apos; . #meanZ);
	Append(FeatureSmiles,&apos;C&apos;);
end if;


</sci:value>
